{
    "version": 2,
    "builds": [
        {
            "src": "backend/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "backend/public/$1"
        }
    ],
    "env": {
        "API_URL": "https://openrouter.ai/api/v1/chat/completions",
        "MODEL": "deepseek/deepseek-r1-0528:free"
    }
} 